<?xml version="1.0" encoding="utf-8"?>
<layer-list xmlns:android="http://schemas.android.com/apk/res/android">

    <item android:left="10dp" android:top="3dp" android:right="3dp">
        <shape android:shape="rectangle">
            <solid android:color="#CCCCCC" />
            <corners android:radius="5dp" />
        </shape>
    </item>

    <item
        android:left="0dp"
        android:top="30dp">
        <rotate
            android:fromDegrees="-45"
            android:toDegrees="0"
            android:pivotX="0%"
            android:pivotY="0%">
            <shape
                android:shape="rectangle" >
                <solid
                    android:color="#CCCCCC"/>
            </shape>
        </rotate>
    </item>

    <item android:left="13dp" android:bottom="3dp">
        <shape android:shape="rectangle">
            <solid android:color="@color/colorWhite" />
            <stroke android:width="1dp" android:color="#CCCCCC"/>
            <corners android:radius="5dp" />
        </shape>
    </item>

    <item
        android:left="3dp"
        android:top="27dp">
        <rotate
            android:fromDegrees="-45"
            android:toDegrees="0"
            android:pivotX="0%"
            android:pivotY="0%">
            <shape
                android:shape="rectangle" >
                <solid
                    android:color="@color/colorWhite"/>
                <stroke android:width="1dp" android:color="#CCCCCC"/>
            </shape>
        </rotate>
    </item>

    <item android:left="20dp" android:right="20dp">
        <rotate
            android:fromDegrees="180"
            android:toDegrees="180"
            android:pivotX="50%"
            android:pivotY="25%" >
            <shape
                android:shape="line" >
                <stroke
                    android:width="2dp"
                    android:color="#CCCCCC"
                    />
            </shape>
        </rotate>
    </item>

    <item
        android:left="14dp"
        android:top="1dp"
        android:right="1dp"
        android:bottom="4dp">
        <rotate
            android:fromDegrees="0"
            android:toDegrees="0"
            android:pivotX="0%"
            android:pivotY="0%">
            <shape
                android:shape="rectangle" >
                <solid
                    android:color="@color/colorWhite"/>
                <corners android:radius="10dp" />
            </shape>
        </rotate>
    </item>


</layer-list>